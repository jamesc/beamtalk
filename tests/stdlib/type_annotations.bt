// Type annotation syntax variations (BT-673)

// @load tests/e2e/fixtures/typed_math.bt

math := TypedMath new
// => _

math identity: 4
// => 4

math identityWithSpace: 5
// => 5

math sum: 2 with: 3
// => 5

math maybeName: true
// => 1

math maybeName: false
// => none

(41 asType: Integer) + 1
// => 42
