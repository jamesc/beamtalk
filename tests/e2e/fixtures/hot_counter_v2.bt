// Hot reload counter - version 2 with modified increment (adds 10)
// and new field 'step'

Actor subclass: HotCounter
  state: value = 0
  state: step = 5

  increment => self.value := self.value + 10
  getValue => ^self.value
  getStep => ^self.step
