typed Object subclass: TypedMath
  identity: value: Integer -> Integer => value

  identityWithSpace: value : Integer -> Integer => value

  + other : Number -> Number => other

  sum: left: Integer with: right: Integer -> Integer => left + right

  maybeName: flag: Boolean -> Integer | String => flag ifTrue: [1] ifFalse: ["none"]
