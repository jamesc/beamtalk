// Point - a simple value type for testing BT-213
// Value types inherit from Object and use 'new', not 'spawn'

Object subclass: Point
  state: x = 0
  state: y = 0

  // Get x coordinate
  getX => self.x

  // Get y coordinate
  getY => self.y
