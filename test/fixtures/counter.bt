// Counter actor class for BUnit testing
// Symlinked/copied from tests/e2e/fixtures/counter.bt

Actor subclass: Counter
  state: value = 0

  increment => self.value := self.value + 1
  decrement => self.value := self.value - 1
  getValue => ^self.value
